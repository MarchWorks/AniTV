{
  "name": "electron-next-skeleton",
  "productName": "ElectronNext",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "dev": "electron .",
    "build": "next build renderer && next export renderer",
    "dist": "npm run build && build --dir",
    "lintfix": "xo --fix"
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-next": "^3.1.5",
    "electron-store": "^3.2.0",
    "fs-extra": "^8.0.1",
    "puppeteer": "^1.16.0",
    "webtorrent": "^0.103.1"
  }
}
